{
    "name": "tsg/progress-bar",
    "description": "A Laravel Nova field to display a variable color progress bar.",
    "keywords": [
        "laravel",
        "nova",
        "progress bar"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Tsg\\ProgressBar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tsg\\ProgressBar\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
